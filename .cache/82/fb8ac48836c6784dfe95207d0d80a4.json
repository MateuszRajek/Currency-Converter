-content: space-around;\r\n    align-items: center;\r\n    flex-grow: 1;\r\n    width: 100%;\r\n}\r\n\r\n.wrap {\r\n    position: relative;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    width: 100%;\r\n}\r\n\r\n.title-of-application {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    padding: 0 25px;\r\n}\r\n\r\n.currency-converter {\r\n    display: flex;\r\n    flex-grow: 1;\r\n    width: 80%;\r\n}\r\n\r\n.circle {\r\n    position: relative;\r\n    align-self: flex-start;\r\n    width: 125px;\r\n    height: 125px;\r\n    background-image: radial-gradient(circle, white 25%, gray 100%);\r\n    border-radius: 50%;\r\n    border: none;\r\n    box-shadow: 0 0 15px gray;\r\n    text-align: center;\r\n}\r\n\r\n.click-to-convert-btn {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    width: 80%;\r\n    height: 80%;\r\n    transform: translate(-50%, -50%);\r\n    background-image: radial-gradient(circle, gray 0%, white 70%);\r\n    box-shadow: 0 0 15px white;\r\n    border-radius: 50%;\r\n    border: none;\r\n    text-transform: uppercase;\r\n    font-size: 18px;\r\n    transition: linear .3s;\r\n    cursor: pointer;\r\n}\r\n\r\n.click-to-convert-btn:hover {\r\n    width: 85%;\r\n    height: 85%;\r\n    background-image: radial-gradient(circle, gray 0%, white 70%);\r\n    font-size: 20px;\r\n}\r\n\r\n.amount-to-convert,\r\n.after-convert-value,\r\n.currency-options {\r\n    width: 80%;\r\n    padding: 10px;\r\n    background-color: white;\r\n    border-radius: 30px;\r\n    border-style: inset;\r\n    border-top-color: rgba(128, 128, 128, .2);\r\n    border-left-color: rgba(128, 128, 128, .2);\r\n    border-bottom: none;\r\n    border-right: none;\r\n    font-size: 24px;\r\n    color: rgb(68, 68, 68);\r\n    flex-shrink: 2;\r\n}\r\n\r\n.currency-options {\r\n    align-self: flex-end;\r\n    width: 120px;\r\n    border-width: 2px;\r\n    border-top-color: rgba(128, 128, 128, .2);\r\n    border-left-color: rgba(128, 128, 128, .2);\r\n    border-bottom: none;\r\n    border-right: none;\r\n    cursor: pointer;\r\n}\r\n\r\n.after-convert-value {\r\n    align-self: flex-end;\r\n}\r\n\r\nlabel {\r\n    position: absolute;\r\n    font-size: 14px;\r\n    color: rgba(46, 49, 49, .6)\r\n}\r\n\r\n.amount-to-convert+label {\r\n    bottom: -15px;\r\n    left: 0;\r\n}\r\n\r\n.currency-options+label {\r\n    bottom: -17px;\r\n    right: 0;\r\n}\r\n\r\n.after-convert-value+label {\r\n    bottom: -15px;\r\n    right: 0;\r\n}\r\n\r\n\r\n@media (max-width: 959px) and (orientation: landscape) {\r\n    .landscape-position-changer {\r\n        position: absolute;\r\n        bottom: 30px;\r\n        right: 20px;\r\n        width: 60%;\r\n    }\r\n\r\n    .amount-to-convert {\r\n        width: 60%;\r\n    }\r\n}\r\n\r\n@media (min-width: 959px) {\r\n    .container {\r\n        height: 65vh;\r\n        width: 50%;\r\n    }\r\n\r\n    .wrapper {\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n        flex-grow: 1;\r\n        width: 80%;\r\n    }\r\n\r\n    .landscape-position-changer,\r\n    .amount-to-convert {\r\n        width: 60%;\r\n    }\r\n\r\n    .landscape-position-changer {\r\n        align-self: flex-end\r\n    }\r\n}\r\n\r\n@media (max-height: 480px) and (orientation: portrait) {\r\n    .container {\r\n        height: 170vh;\r\n    }\r\n}\r\n\r\n@media (max-height: 280px) and (orientation: landscape) {\r\n    .container {\r\n        height: 300vh;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"8fe4bd9ca022614c6fd5254eeadf2704","cacheData":{"env":{}}}